<h1>All Artworks</h1>
<% allArtworks.forEach(artwork => { %>
    <div>
        <h3><%= artwork.title %></h3>
        <p>By: <%= artwork.artist.username %></p>
       <img src="<%= artwork.image %>" alt="<%= artwork.title %>" width="300">
        <a href="/artworks/<%= artwork._id %>">View Details</a>
    </div>
<% }) %>
